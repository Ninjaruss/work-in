{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "client-install": "cd client && yarn install",
    "start": "yarn ./bin/www",
    "server": "nodemon ./bin/www",
    "build": "cd ../frontend && yarn install && yarn run build",
    "client": "cd ../frontend && yarn start",
    "dev": "concurrently \"yarn run server\" \"yarn run client\" "
  },
  "dependencies": {
    "concurrently": "^7.5.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^6.7.1",
    "morgan": "~1.9.1",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
